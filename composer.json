{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "jm-sky/saasbase-api",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "sass"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "doctrine/dbal": "^4.2",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.31",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.20",
        "laravel/telescope": "^5.7",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "lorisleiva/laravel-actions": "^2.9",
        "pragmarx/google2fa": "^8.0",
        "ramsey/uuid": "^4.7",
        "rap2hpoutre/laravel-log-viewer": "^2.5",
        "saloonphp/saloon": "^3.11",
        "spatie/laravel-data": "^4.15",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-query-builder": "^6.3",
        "tymon/jwt-auth": "^2.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.24",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "larastan/larastan": "^3.3",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" --names=server,queue,logs"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "cs": [
            "./vendor/bin/php-cs-fixer fix --allow-risky=yes --dry-run --diff --config codestyle.php"
        ],
        "csf": [
            "./vendor/bin/php-cs-fixer fix --allow-risky=yes --diff --config codestyle.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
